#include <iostream>

ColaM::ColaM(){
	mem=new CSMemoria();
	int ini=-1;
	int fin=-1;
	mem->mostrar_memoria();    //test
}

bool ColaM::Vacia(){
	return ini==NULO;
}

void ColaM::Poner(int E){
	int aux= 0; //Pedir Espacio de memoria para Nodo
	if (aux!=NULO) {
		mem->poner_dato(aux,datoC,elementoC);
		mem->poner_dato(aux,sigC,NULO);
		if (Vacia()) {
			ini=aux;
			fin=aux;
		}else{
			mem->poner_dato(fin,sigC,aux);
			fin=aux;
		}
	}else{
		cout<<"Error\n";
    }
}

